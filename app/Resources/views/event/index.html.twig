<h1> {{ events|length }} Events</h1>

{% for event in events %}
    <article>
        <h2>{{ event.name }}</h2>
        <p>{{ event.description|truncate(default_truncate_limit, true) }}</p>
        <ul>
            <li>{{ event.Location }}</li>
            <li>{{ event.startsAt | date }}</li>
            <li>${{ event.price }}</li>
        </ul>
    </article>
{% endfor %}

